    "nltk.download('vader_lexicon')\n",
    "from nltk.sentiment.vader import SentimentIntensityAnalyzer\n",
    "\n",
    "# initialize the VADER sentiment analyzer\n",
    "analyzer = SentimentIntensityAnalyzer()\n",
    "\n",
    "# Define a function to classify headlines based on their sentiment score\n",
    "def classify_headline(headline, threshold=0):\n",
    "    score = analyzer.polarity_scores(str(headline))['compound']\n",
    "    return 1 if score >= threshold else 0\n",
    "\n",
    "# Apply the classification function to each headline in the dataset\n",
    "for col in df.columns:\n",
    "    df[col] = df[col].apply(lambda x: classify_headline(x, threshold=0.1))\n",
    "df"
   ]
